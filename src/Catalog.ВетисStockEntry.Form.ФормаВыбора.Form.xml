	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ref</Field>
				<Field>Список.Enterprise</Field>
				<Field>Список.BusinessEntity</Field>
				<Field>~Список.Unit</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	_Таблица.BusinessEntity,
	_Таблица.Enterprise,
	_Таблица.Ссылка,
	_Таблица.Код,
	_Таблица.Наименование,
	_Таблица.volume - ЕСТЬNULL(_Транзакции.Количество, 0) КАК Количество,
	_Таблица.producerName,
	_Таблица.country,
	_Таблица.productMarking,
	_Таблица.dateOfProduction,
	_Таблица.expiryDate,
	_Таблица.expiryDateFirst,
	_Таблица.expiryDateSecond
ИЗ
	Справочник.ВетисStockEntry КАК _Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			_Таблица.StockEntry КАК Ссылка,
			СУММА(_Таблица.Количество) КАК Количество
		ИЗ
			Документ.ВетисТранспортнаяПартия.Партии КАК _Таблица
		ГДЕ
			ИСТИНА
			И НЕ _Таблица.Ссылка.ПометкаУдаления
			И (_Таблица.Ссылка.СтатусТранзакции = ЗНАЧЕНИЕ(Перечисление.ВетисСтатусТранзакции.ПустаяСсылка)
					ИЛИ _Таблица.Ссылка.СтатусТранзакции = ЗНАЧЕНИЕ(Перечисление.ВетисСтатусТранзакции.НеОтправлена))
			И _Таблица.Ссылка.Дата &gt;= &amp;Дата
			И НЕ _Таблица.Ссылка = &amp;ТекущийДокумент
		
		СГРУППИРОВАТЬ ПО
			_Таблица.StockEntry) КАК _Транзакции
		ПО _Таблица.Ссылка = _Транзакции.Ссылка
ГДЕ
	ИСТИНА
	И _Таблица.last
	И _Таблица.active
	И _Таблица.volume &gt; ЕСТЬNULL(_Транзакции.Количество, 0)</QueryText>
				<MainTable>Catalog.ВетисStockEntry</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
